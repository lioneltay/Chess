{
  "name": "chess-from-scratch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "typecheck": "tsc --noEmit --watch --skipLibCheck",
    "dev": "webpack-dev-server --config ./webpack/webpack.config.dev.js",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --config ./webpack/webpack.config.prod.js",
    "deploy:dev": "npm run build && gcloud app deploy dev.yaml --project=lioneltay --quiet && gcloud app deploy dispatch.yaml --quiet",
    "deploy:prod": "npm run build && gcloud app deploy prod.yaml --project=lioneltay --quiet && gcloud app deploy dispatch.yaml --quiet"
  },
  "author": "",
  "license": "ISC",
